    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/54/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/54/ayat/1/point/b> a legal:Point ;
    legal:hasContent "nama, jenis kelamin, umur, dan alamat pekerja/buruh;"^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/54/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/54/ayat/1/point/c> a legal:Point ;
    legal:hasContent "jabatan atau jenis pekerjaan;"^^xsd:string ;
    legal:hasKey "c"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/54/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/54/ayat/1/point/d> a legal:Point ;
    legal:hasContent "tempat pekerjaan;"^^xsd:string ;
    legal:hasKey "d"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/54/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/54/ayat/1/point/e> a legal:Point ;
