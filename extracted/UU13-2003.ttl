@prefix : <http://example.org/legal/document/> .
@prefix legal: <http://example.org/legal/ontology/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/legal/document/UU13-2003/mengingat> legal:mengingat "Pasal 5 ayat (1), Pasal 20 ayat (2), Pasal 27 ayat (2), Pasal 28, dan Pasal 33 ayat (1) Undang Undang Dasar Negara Republik Indonesia Tahun 1945;"^^xsd:string .

<http://example.org/legal/document/UU13-2003/menimbang/point/a> a legal:Point ;
    legal:hasContent "bahwa pembangunan nasional dilaksanakan dalam rangka pembangunan manusia Indonesia seutuhnya dan pembangunan masyarakat Indonesia seluruhnya untuk mewujudkan masyarakat yang sejahtera, adil, makmur, yang merata, baik materiil maupun spiritual berdasarkan Pancasila dan Undang Undang Dasar Negara Republik Indonesia Tahun 1945;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/menimbang> .

<http://example.org/legal/document/UU13-2003/menimbang/point/b> a legal:Point ;
    legal:hasContent "bahwa dalam pelaksanaan pembangunan nasional, tenaga kerja mempunyai peranan dan kedudukan yang sangat penting sebagai pelaku dan tujuan pembangunan;"^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/menimbang> .

<http://example.org/legal/document/UU13-2003/menimbang/point/c> a legal:Point ;
    legal:hasContent "bahwa sesuai dengan peranan dan kedudukan tenaga kerja, diperlukan pembangunan ketenagakerjaan untuk meningkatkan kualitas tenaga kerja dan peransertanya dalam pembangunan serta peningkatan perlindungan tenaga kerja dan keluarganya sesuai dengan harkat dan martabat kemanusiaan;"^^xsd:string ;
    legal:hasKey "c"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/menimbang> .

<http://example.org/legal/document/UU13-2003/menimbang/point/d> a legal:Point ;
    legal:hasContent "bahwa perlindungan terhadap tenaga kerja dimaksudkan untuk menjamin hak hak dasar pekerja/buruh dan menjamin kesamaan kesempatan serta perlakuan tanpa diskriminasi atas dasar apapun untuk mewujudkan kesejahteraan pekerja/buruh dan keluarganya dengan tetap memperhatikan perkembangan kemajuan dunia usaha;"^^xsd:string ;
    legal:hasKey "d"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/menimbang> .

<http://example.org/legal/document/UU13-2003/menimbang/point/e> a legal:Point ;
    legal:hasContent "bahwa beberapa undang undang di bidang ketenagakerjaan dipandang sudah tidak sesuai lagi dengan kebutuhan dan tuntutan pembangunan ketenagakerjaan, oleh karena itu perlu dicabut dan/atau ditarik kembali;"^^xsd:string ;
    legal:hasKey "e"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/menimbang> .

<http://example.org/legal/document/UU13-2003/menimbang/point/f> a legal:Point ;
    legal:hasContent "bahwa berdasarkan pertimbangan sebagaimana tersebut pada huruf a, b, c, d, dan e perlu membentuk Undang undang tentang Ketenagakerjaan;"^^xsd:string ;
    legal:hasKey "f"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/menimbang> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/1> a legal:Point ;
    legal:hasContent "Ketenagakerjaan adalah segala hal yang berhubungan dengan tenaga kerja pada waktu sebelum, selama, dan sesudah masa kerja."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/10> a legal:Point ;
    legal:hasContent "Kompetensi kerja adalah kemampuan kerja setiap individu yang mencakup aspek pengetahuan, keterampilan, dan sikap kerja yang sesuai dengan standar yang ditetapkan."^^xsd:string ;
    legal:hasKey 10 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/11> a legal:Point ;
    legal:hasContent "Pemagangan adalah bagian dari sistem pelatihan kerja yang diselenggarakan secara terpadu antara pelatihan di lembaga pelatihan dengan bekerja secara langsung di bawah bimbingan dan pengawasan instruktur atau pekerja/buruh yang lebih berpengalaman, dalam proses produksi barang dan/atau jasa di perusahaan, dalam rangka menguasai keterampilan atau keahlian tertentu."^^xsd:string ;
    legal:hasKey 11 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/12> a legal:Point ;
    legal:hasContent "Pelayanan penempatan tenaga kerja adalah kegiatan untuk mempertemukan tenaga kerja dengan pemberi kerja, sehingga tenaga kerja dapat memperoleh pekerjaan yang sesuai dengan bakat, minat, dan kemampuannya, dan pemberi kerja dapat memperoleh tenaga kerja yang sesuai dengan kebutuhannya."^^xsd:string ;
    legal:hasKey 12 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/13> a legal:Point ;
    legal:hasContent "Tenaga kerja asing adalah warga negara asing pemegang visa dengan maksud bekerja di wilayah Indonesia."^^xsd:string ;
    legal:hasKey 13 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/14> a legal:Point ;
    legal:hasContent "Perjanjian kerja adalah perjanjian antara pekerja/buruh dengan pengusaha atau pemberi kerja yang memuat syarat syarat kerja, hak, dan kewajiban para pihak."^^xsd:string ;
    legal:hasKey 14 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/15> a legal:Point ;
    legal:hasContent "Hubungan kerja adalah hubungan antara pengusaha dengan pekerja/buruh berdasarkan perjanjian kerja, yang mempunyai unsur pekerjaan, upah, dan perintah."^^xsd:string ;
    legal:hasKey 15 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/16> a legal:Point ;
    legal:hasContent "Hubungan industrial adalah suatu sistem hubungan yang terbentuk antara para pelaku dalam proses produksi barang dan/atau jasa yang terdiri dari unsur pengusaha, pekerja/buruh, dan pemerintah yang didasarkan pada nilai nilai Pancasila dan Undang Undang Dasar Negara Republik Indonesia Tahun 1945."^^xsd:string ;
    legal:hasKey 16 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/17> a legal:Point ;
    legal:hasContent "Serikat pekerja/serikat buruh adalah organisasi yang dibentuk dari, oleh, dan untuk pekerja/buruh baik di perusahaan maupun di luar perusahaan, yang bersifat bebas, terbuka, mandiri, demokratis, dan bertanggung jawab guna memperjuangkan, membela serta melindungi hak dan kepentingan pekerja/buruh serta meningkatkan kesejahteraan pekerja/buruh dan keluarganya."^^xsd:string ;
    legal:hasKey 17 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/18> a legal:Point ;
    legal:hasContent "Lembaga kerja sama bipartit adalah forum komunikasi dan konsultasi mengenai hal-hal yang berkaitan dengan hubungan industrial di satu perusahaan yang anggotanya terdiri dari pengusaha dan serikat pekerja/ serikat buruh yang sudah tercatat instansi yang bertanggung jawab di bidang ketenagakerjaan atau unsur pekerja/buruh."^^xsd:string ;
    legal:hasKey 18 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/19> a legal:Point ;
    legal:hasContent "Lembaga kerja sama tripartit adalah forum komunikasi, konsultasi dan musyawarah tentang masalah ketenagakerjaan yang anggotanya terdiri dari unsur organisasi pengusaha, serikat pekerja/serikat buruh, dan pemerintah."^^xsd:string ;
    legal:hasKey 19 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/2> a legal:Point ;
    legal:hasContent "Tenaga kerja adalah setiap orang yang mampu melakukan pekerjaan guna menghasilkan barang dan/atau jasa baik untuk memenuhi kebutuhan sendiri maupun untuk masyarakat."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/20> a legal:Point ;
    legal:hasContent "Peraturan perusahaan adalah peraturan yang dibuat secara tertulis oleh pengusaha yang memuat syarat syarat kerja dan tata tertib perusahaan."^^xsd:string ;
    legal:hasKey 20 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/21> a legal:Point ;
    legal:hasContent "Perjanjian kerja bersama adalah perjanjian yang merupakan hasil perundingan antara serikat pekerja/serikat buruh atau beberapa serikat pekerja/serikat buruh yang tercatat pada instansi yang bertanggung jawab di bidang ketenagakerjaan dengan pengusaha, atau beberapa pengusaha atau perkumpulan pengusaha yang memuat syarat syarat kerja, hak dan kewajiban kedua belah pihak."^^xsd:string ;
    legal:hasKey 21 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/22> a legal:Point ;
    legal:hasContent "Perselisihan hubungan industrial adalah perbedaan pendapat yang mengakibatkan pertentangan antara pengusaha atau gabungan pengusaha dengan pekerja/buruh atau serikat pekerja/serikat buruh karena adanya perselisihan mengenai hak, perselisihan kepentingan, dan perselisihan pemutusan hubungan kerja serta perselisihan antar serikat pekerja/serikat buruh hanya dalam satu perusahaan."^^xsd:string ;
    legal:hasKey 22 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/23> a legal:Point ;
    legal:hasContent "Mogok kerja adalah tindakan pekerja/buruh yang direncanakan dan dilaksanakan secara bersama-sama dan/atau oleh serikat pekerja/serikat buruh untuk menghentikan atau memperlambat pekerjaan."^^xsd:string ;
    legal:hasKey 23 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/24> a legal:Point ;
    legal:hasContent "Penutupan perusahaan (lock out) adalah tindakan pengusaha untuk menolak pekerja/buruh seluruhnya atau sebagian untuk menjalankan pekerjaan."^^xsd:string ;
    legal:hasKey 24 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/25> a legal:Point ;
    legal:hasContent "Pemutusan hubungan kerja adalah pengakhiran hubungan kerja karena suatu hal tertentu yang mengakibatkan berakhirnya hak dan kewajiban antara pekerja/buruh dan pengusaha."^^xsd:string ;
    legal:hasKey 25 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/26> a legal:Point ;
    legal:hasContent "Anak adalah setiap orang yang berumur dibawah 18 (delapan belas) tahun."^^xsd:string ;
    legal:hasKey 26 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/27> a legal:Point ;
    legal:hasContent "Siang hari adalah waktu antara pukul 06.00 sampai dengan pukul 18.00."^^xsd:string ;
    legal:hasKey 27 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/28> a legal:Point ;
    legal:hasContent "1 (satu) hari adalah waktu selama 24 (dua puluh empat) jam."^^xsd:string ;
    legal:hasKey 28 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/29> a legal:Point ;
    legal:hasContent "Seminggu adalah waktu selama 7 (tujuh) hari."^^xsd:string ;
    legal:hasKey 29 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/3> a legal:Point ;
    legal:hasContent "Pekerja/buruh adalah setiap orang yang bekerja dengan menerima upah atau imbalan dalam bentuk lain."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/30> a legal:Point ;
    legal:hasContent "Upah adalah hak pekerja/buruh yang diterima dan dinyatakan dalam bentuk uang sebagai imbalan dari pengusaha atau pemberi kerja kepada pekerja/buruh yang ditetapkan dan dibayarkan menurut suatu perjanjian kerja, kesepakatan, atau peraturan perundang undangan, termasuk tunjangan bagi pekerja/buruh dan keluarganya atas suatu pekerjaan dan/atau jasa yang telah atau akan dilakukan."^^xsd:string ;
    legal:hasKey 30 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/31> a legal:Point ;
    legal:hasContent "31. Kesejahteraan pekerja/buruh adalah suatu pemenuhan kebutuhan dan/atau keperluan yang bersifat jasmaniah dan rohaniah, baik di dalam maupun di luar hubungan kerja, yang secara langsung atau tidak langsung dapat mempertinggi produktivitas kerja dalam lingkungan kerja yang aman dan sehat."^^xsd:string ;
    legal:hasKey 31 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/32> a legal:Point ;
    legal:hasContent "Pengawasan ketenagakerjaan adalah kegiatan mengawasi dan menegakkan pelaksanaan peraturan perundang undangan di bidang ketenagakerjaan."^^xsd:string ;
    legal:hasKey 32 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/33> a legal:Point ;
    legal:hasContent "Menteri adalah menteri yang bertanggung jawab di bidang ketenagakerjaan."^^xsd:string ;
    legal:hasKey 33 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/4> a legal:Point ;
    legal:hasContent "Pemberi kerja adalah orang perseorangan, pengusaha, badan hukum, atau badan-badan lainnya yang mempekerjakan tenaga kerja dengan membayar upah atau imbalan dalam bentuk lain."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/5/point/a> a legal:Point ;
    legal:hasContent "orang perseorangan, persekutuan, atau badan hukum yang menjalankan suatu perusahaan milik sendiri;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1/point/5> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/5/point/b> a legal:Point ;
    legal:hasContent "orang perseorangan, persekutuan, atau badan hukum yang secara berdiri sendiri menjalankan perusahaan bukan miliknya;"^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1/point/5> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/5/point/c> a legal:Point ;
    legal:hasContent "orang perseorangan, persekutuan, atau badan hukum yang berada di Indonesia mewakili perusahaan sebagaimana dimaksud dalam huruf a dan b yang berkedudukan di luar wilayah Indonesia."^^xsd:string ;
    legal:hasKey "c"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1/point/5> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/6/point/a> a legal:Point ;
    legal:hasContent "setiap bentuk usaha yang berbadan hukum atau tidak, milik orang perseorangan, milik persekutuan, atau milik badan hukum, baik milik swasta maupun milik negara yang mempekerjakan pekerja/buruh dengan membayar upah atau imbalan dalam bentuk lain;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1/point/6> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/6/point/b> a legal:Point ;
    legal:hasContent "usaha-usaha sosial dan usaha-usaha lain yang mempunyai pengurus dan mempekerjakan orang lain dengan membayar upah atau imbalan dalam bentuk lain."^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1/point/6> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/7> a legal:Point ;
    legal:hasContent "Perencanaan tenaga kerja adalah proses penyusunan rencana ketenagakerjaan secara sistematis yang dijadikan dasar dan acuan dalam penyusunan kebijakan, strategi, dan pelaksanaan program pembangunan ketenagakerjaan yang berkesinambungan."^^xsd:string ;
    legal:hasKey 7 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/8> a legal:Point ;
    legal:hasContent "Informasi ketenagakerjaan adalah gabungan, rangkaian, dan analisis data yang berbentuk angka yang telah diolah, naskah dan dokumen yang mempunyai arti, nilai dan makna tertentu mengenai ketenagakerjaan."^^xsd:string ;
    legal:hasKey 8 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/1/point/9> a legal:Point ;
    legal:hasContent "Pelatihan kerja adalah keseluruhan kegiatan untuk memberi, memperoleh, meningkatkan, serta mengembangkan kompetensi kerja, produktivitas, disiplin, sikap, dan etos kerja pada tingkat keterampilan dan keahlian tertentu sesuai dengan jenjang dan kualifikasi jabatan atau pekerjaan."^^xsd:string ;
    legal:hasKey 9 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/1> .

<http://example.org/legal/document/UU13-2003/pasal/10/ayat/1> a legal:Ayat ;
    legal:hasContent "Pelatihan kerja dilaksanakan dengan memperhatikan kebutuhan pasar kerja dan dunia usaha, baik di da-lam maupun di luar hubungan kerja."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/10> .

<http://example.org/legal/document/UU13-2003/pasal/10/ayat/2> a legal:Ayat ;
    legal:hasContent "Pelatihan kerja diselenggarakan berdasarkan program pelatihan yang mengacu pada standar kompetensi kerja."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/10> .

<http://example.org/legal/document/UU13-2003/pasal/10/ayat/3> a legal:Ayat ;
    legal:hasContent "Pelatihan kerja dapat dilakukan secara berjenjang."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/10> .

<http://example.org/legal/document/UU13-2003/pasal/10/ayat/4> a legal:Ayat ;
    legal:hasContent "Ketentuan mengenai tata cara penetapan standar kompetensi kerja sebagaimana dimaksud dalam ayat (2) diatur dengan Keputusan Menteri."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/10> .

<http://example.org/legal/document/UU13-2003/pasal/100/ayat/1> a legal:Ayat ;
    legal:hasContent "Untuk meningkatkan kesejahteraan bagi pekerja/buruh dan keluarganya, pengusaha wajib menyediakan fasilitas kesejahteraan."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/100> .

<http://example.org/legal/document/UU13-2003/pasal/100/ayat/2> a legal:Ayat ;
    legal:hasContent "Penyediaan fasilitas kesejahteraan sebagaimana dimaksud dalam ayat (1), dilak?sanakan dengan  memperhatikan kebutuhan pekerja/buruh dan ukuran kemampuan perusa?haan."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/100> .

<http://example.org/legal/document/UU13-2003/pasal/100/ayat/3> a legal:Ayat ;
    legal:hasContent "Ketentuan mengenai jenis dan kriteria fasilitas kesejahteraan sesuai dengan kebutuhan pekerja/buruh dan ukuran kemampuan perusahaan sebagaimana dimaksud dalam ayat (1) dan ayat (2), diatur dengan Peraturan Pemerintah."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/100> .

<http://example.org/legal/document/UU13-2003/pasal/101/ayat/1> a legal:Ayat ;
    legal:hasContent "Untuk meningkatkan kesejahteraan pekerja/buruh, dibentuk koperasi pekerja/buruh dan usaha- usaha produktif di perusahaan."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/101> .

<http://example.org/legal/document/UU13-2003/pasal/101/ayat/2> a legal:Ayat ;
    legal:hasContent "Pemerintah, pengusaha, dan pekerja/buruh atau serikat pekerja/serikat buruh berupaya menumbuhkembangkan koperasi pekerja/buruh, dan mengembangkan usaha produktif sebagaimana dimaksud dalam ayat (1)."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/101> .

<http://example.org/legal/document/UU13-2003/pasal/101/ayat/3> a legal:Ayat ;
    legal:hasContent "Pembentukan koperasi sebagaimana dimaksud dalam ayat (1), dilaksanakan sesuai dengan peraturan perundang-undangan yang berlaku."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/101> .

<http://example.org/legal/document/UU13-2003/pasal/101/ayat/4> a legal:Ayat ;
    legal:hasContent "Upaya-upaya untuk menumbuhkembangkan koperasi pekerja/buruh sebagaimana dimaksud dalam ayat (2), diatur dengan Peraturan Pemerintah."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/101> .

<http://example.org/legal/document/UU13-2003/pasal/102/ayat/1> a legal:Ayat ;
    legal:hasContent "Dalam melaksanakan hubungan industrial, pemerintah mempunyai fungsi menetapkan kebijakan, memberikan pelayanan, melaksanakan pengawasan, dan melakukan penindakan terhadap pelanggaran peraturan perundang-undangan ketenagakerjaan."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/102> .

<http://example.org/legal/document/UU13-2003/pasal/102/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam melaksanakan hubungan industrial, pekerja/buruh dan serikat pekerja/serikat buruhnya mempunyai fungsi menjalankan pekerjaan sesuai dengan kewajibannya, menjaga ketertiban demi kelangsungan produksi, menyalurkan aspirasi secara demokratis, mengembangkan keterampilan, dan keahliannya serta ikut memajukan perusahaan dan memperjuangkan kesejahteraan anggota beserta keluarganya."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/102> .

<http://example.org/legal/document/UU13-2003/pasal/102/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam melaksanakan hubungan industrial, pengusaha dan organisasi pengusahanya mempunyai fungsi menciptakan kemitraan, mengembang-kan usaha, memperluas lapangan kerja, dan memberikan kesejahteraan pekerja/buruh secara terbuka, demokratis, dan berkeadilan."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/102> .

<http://example.org/legal/document/UU13-2003/pasal/103/point/a> a legal:Point ;
    legal:hasContent "serikat pekerja/serikat buruh;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/103> .

<http://example.org/legal/document/UU13-2003/pasal/103/point/b> a legal:Point ;
    legal:hasContent "organisasi pengusaha;"^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/103> .

<http://example.org/legal/document/UU13-2003/pasal/103/point/c> a legal:Point ;
    legal:hasContent "lembaga kerja sama bipartit;"^^xsd:string ;
    legal:hasKey "c"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/103> .

<http://example.org/legal/document/UU13-2003/pasal/103/point/d> a legal:Point ;
    legal:hasContent "embaga kerja sama tripartit;"^^xsd:string ;
    legal:hasKey "d"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/103> .

<http://example.org/legal/document/UU13-2003/pasal/103/point/e> a legal:Point ;
    legal:hasContent "peraturan perusahaan;"^^xsd:string ;
    legal:hasKey "e"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/103> .

<http://example.org/legal/document/UU13-2003/pasal/103/point/f> a legal:Point ;
    legal:hasContent "perjanjian kerja bersama;"^^xsd:string ;
    legal:hasKey "f"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/103> .

<http://example.org/legal/document/UU13-2003/pasal/103/point/g> a legal:Point ;
    legal:hasContent "peraturan perundang-undangan ketenagakerjaan; dan"^^xsd:string ;
    legal:hasKey "g"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/103> .

<http://example.org/legal/document/UU13-2003/pasal/103/point/h> a legal:Point ;
    legal:hasContent "lembaga penyelesaian perselisihan hubungan industrial."^^xsd:string ;
    legal:hasKey "h"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/103> .

<http://example.org/legal/document/UU13-2003/pasal/104/ayat/1> a legal:Ayat ;
    legal:hasContent "Setiap pekerja/buruh berhak membentuk dan menjadi anggota serikat pekerja/serikat buruh."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/104> .

<http://example.org/legal/document/UU13-2003/pasal/104/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam melaksanakan fungsi sebagaimana dimaksud dalam Pasal 102, serikat pekerja/serikat buruh ber-hak menghimpun dan mengelola keuangan serta mempertanggungjawabkan keuangan organisasi termasuk dana mogok."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/104> .

<http://example.org/legal/document/UU13-2003/pasal/104/ayat/3> a legal:Ayat ;
    legal:hasContent "Besarnya dan tata cara pemungutan dana mogok sebagaimana dimaksud dalam ayat (2) diatur dalam ang-garan dasar dan/atau anggaran rumah tangga serikat pekerja/serikat buruh yang bersangkutan."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/104> .

<http://example.org/legal/document/UU13-2003/pasal/105/ayat/1> a legal:Ayat ;
    legal:hasContent "Setiap pengusaha berhak membentuk dan menjadi anggota organisasi pengusaha."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/105> .

<http://example.org/legal/document/UU13-2003/pasal/105/ayat/2> a legal:Ayat ;
    legal:hasContent "Ketentuan mengenai organisasi pengusaha diatur sesuai dengan peraturan perundang-undangan yang ber-laku."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/105> .

<http://example.org/legal/document/UU13-2003/pasal/106/ayat/1> a legal:Ayat ;
    legal:hasContent "Setiap perusahaan yang mempekerjakan 50 (lima puluh) orang pekerja/ buruh atau lebih wajib membentuk lembaga kerja sama bipartit."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/106> .

<http://example.org/legal/document/UU13-2003/pasal/106/ayat/2> a legal:Ayat ;
    legal:hasContent "Lembaga kerja sama bipartit sebagaimana dimaksud dalam ayat (1) berfungsi sebagai forum komunikasi, dan konsultasi mengenai hal ketenagakerjaan di perusahaan."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/106> .

<http://example.org/legal/document/UU13-2003/pasal/106/ayat/3> a legal:Ayat ;
    legal:hasContent "Susunan keanggotaan lembaga kerja sama bipartit sebagaimana dimaksud dalam ayat (2) terdiri dari unsur pengusaha dan unsur pekerja/buruh yang ditunjuk oleh pekerja/buruh secara demokratis untuk mewakili kepentingan pekerja/buruh di perusahaan yang bersangkutan."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/106> .

<http://example.org/legal/document/UU13-2003/pasal/106/ayat/4> a legal:Ayat ;
    legal:hasContent "Ketentuan mengenai tata cara pembentukan dan susunan keanggotaan lembaga kerja sama bipartit sebagaimana dimaksud dalam ayat (1) dan ayat (3) diatur dengan Keputusan Menteri."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/106> .

<http://example.org/legal/document/UU13-2003/pasal/107/ayat/1> a legal:Ayat ;
    legal:hasContent "Lembaga kerja sama tripartit memberikan pertimbangan, saran, dan pendapat kepada pemerintah dan pihak terkait dalam penyusunan kebijakan dan pemecahan masalah ketenagakerjaan."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/107> .

<http://example.org/legal/document/UU13-2003/pasal/107/ayat/2/point/a> a legal:Point ;
    legal:hasContent "Lembaga Kerja sama Tripartit Nasional, Provinsi, dan Kabupaten/ Kota; dan"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/107/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/107/ayat/2/point/b> a legal:Point ;
    legal:hasContent "Lembaga Kerja sama Tripartit Sektoral Nasional, Provinsi, dan Kabupaten/Kota."^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/107/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/107/ayat/3> a legal:Ayat ;
    legal:hasContent "Keanggotaan Lembaga Kerja sama Tripartit terdiri dari unsur pemerintah, organisasi pengusaha, dan seri-kat pekerja/serikat buruh."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/107> .

<http://example.org/legal/document/UU13-2003/pasal/107/ayat/4> a legal:Ayat ;
    legal:hasContent "Tata kerja dan susunan organisasi Lembaga Kerja sama Tripartit sebagaimana dimaksud dalam ayat (1) diatur dengan Peraturan Pemerintah."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/107> .

<http://example.org/legal/document/UU13-2003/pasal/108/ayat/1> a legal:Ayat ;
    legal:hasContent "Pengusaha yang mempekerjakan pekerja/buruh sekurang-kurangnya 10 (sepuluh) orang wajib membuat peraturan perusahaan yang mulai berlaku setelah disahkan oleh Menteri atau pejabat yang ditunjuk."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/108> .

<http://example.org/legal/document/UU13-2003/pasal/108/ayat/2> a legal:Ayat ;
    legal:hasContent "Kewajiban membuat peraturan perusahaan sebagaimana dimaksud dalam ayat (1) tidak berlaku bagi peru-sahaan yang telah memiliki perjanjian kerja bersama."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/108> .

<http://example.org/legal/document/UU13-2003/pasal/109> a legal:Pasal ;
    legal:hasContent "Peraturan perusahaan disusun oleh dan menjadi tanggung jawab dari pengusaha yang bersangkutan."^^xsd:string ;
    legal:hasKey 109 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/6> .

<http://example.org/legal/document/UU13-2003/pasal/11> a legal:Pasal ;
    legal:hasContent "Setiap tenaga kerja berhak untuk memperoleh dan/atau meningkatkan dan/atau mengembangkan kompetensi kerja sesuai dengan bakat, minat, dan kemampuannya melalui pelatihan kerja."^^xsd:string ;
    legal:hasKey 11 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/5> .

<http://example.org/legal/document/UU13-2003/pasal/110/ayat/1> a legal:Ayat ;
    legal:hasContent "Peraturan perusahaan disusun dengan memperhatikan saran dan pertimbangan dari wakil pekerja/buruh di perusahaan yang bersangkutan."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/110> .

<http://example.org/legal/document/UU13-2003/pasal/110/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam hal di perusahaan yang bersangkutan telah terbentuk serikat pekerja/serikat buruh maka wakil pe-kerja/buruh sebagaimana dimaksud dalam ayat (1) adalah pengurus serikat pekerja/serikat buruh."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/110> .

<http://example.org/legal/document/UU13-2003/pasal/110/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal di perusahaan yang bersangkutan belum terbentuk serikat pekerja/serikat buruh, wakil pekerja/ buruh sebagaimana dimaksud dalam ayat (1) adalah pekerja/buruh yang dipilih secara demokratis untuk mewakili kepentingan para pekerja/buruh di perusahaan yang bersangkutan."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/110> .

<http://example.org/legal/document/UU13-2003/pasal/111/ayat/1/point/a> a legal:Point ;
    legal:hasContent "hak dan kewajiban pengusaha;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/111/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/111/ayat/1/point/b> a legal:Point ;
    legal:hasContent "hak dan kewajiban pekerja/buruh;"^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/111/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/111/ayat/1/point/c> a legal:Point ;
    legal:hasContent "syarat kerja;"^^xsd:string ;
    legal:hasKey "c"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/111/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/111/ayat/1/point/d> a legal:Point ;
    legal:hasContent "tata tertib perusahaan; dan"^^xsd:string ;
    legal:hasKey "d"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/111/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/111/ayat/1/point/e> a legal:Point ;
    legal:hasContent "jangka waktu berlakunya peraturan perusahaan."^^xsd:string ;
    legal:hasKey "e"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/111/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/111/ayat/2> a legal:Ayat ;
    legal:hasContent "Ketentuan dalam peraturan perusahaan tidak boleh bertentangan dengan ketentuan peraturan perundang undangan yang berlaku."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/111> .

<http://example.org/legal/document/UU13-2003/pasal/111/ayat/3> a legal:Ayat ;
    legal:hasContent "Masa berlaku peraturan perusahaan paling lama 2 (dua) tahun dan wajib diperbaharui setelah habis masa berlakunya."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/111> .

<http://example.org/legal/document/UU13-2003/pasal/111/ayat/4> a legal:Ayat ;
    legal:hasContent "Selama masa berlakunya peraturan perusahaan, apabila serikat pekerja/ serikat buruh di perusahaan meng hendaki perundingan pembuatan perjanjian kerja bersama, maka pengusaha wajib melayani."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/111> .

<http://example.org/legal/document/UU13-2003/pasal/111/ayat/5> a legal:Ayat ;
    legal:hasContent "Dalam hal perundingan pembuatan perjanjian kerja bersama sebagaimana dimaksud dalam ayat (4) tidak mencapai kesepakatan, maka peraturan perusahaan tetap berlaku sampai habis jangka waktu berlakunya."^^xsd:string ;
    legal:hasKey 5 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/111> .

<http://example.org/legal/document/UU13-2003/pasal/112/ayat/1> a legal:Ayat ;
    legal:hasContent "Pengesahan peraturan perusahaan oleh Menteri atau pejabat yang ditunjuk sebagaimana dimaksud dalam Pasal 108 ayat (1) harus sudah diberikan dalam waktu paling lama 30 (tiga puluh) hari kerja sejak naskah peraturan perusahaan diterima."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/112> .

<http://example.org/legal/document/UU13-2003/pasal/112/ayat/2> a legal:Ayat ;
    legal:hasContent "Apabila peraturan perusahaan telah sesuai sebagaimana ketentuan dalam Pasal 111 ayat (1) dan ayat (2), maka dalam waktu 30 (tiga puluh) hari kerja sebagaimana dimaksud pada ayat (1) sudah terlampaui dan peraturan perusahaan belum disahkan oleh Menteri atau pejabat yang ditunjuk, maka peraturan perusahaan dianggap telah mendapatkan pengesahan."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/112> .

<http://example.org/legal/document/UU13-2003/pasal/112/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal peraturan perusahaan belum memenuhi persyaratan sebagaimana dimaksud dalam Pasal 111 ayat (1) dan ayat (2) Menteri atau pejabat yang ditunjuk harus memberitahukan secara tertulis kepada pengusaha mengenai perbaikan peraturan perusahaan."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/112> .

<http://example.org/legal/document/UU13-2003/pasal/112/ayat/4> a legal:Ayat ;
    legal:hasContent "Dalam waktu paling lama 14 (empat belas) hari kerja sejak tanggal pemberitahuan diterima oleh pengusaha sebagaimana dimaksud dalam ayat (3), pengusaha wajib menyampaikan kembali peraturan perusahaan yang telah diperbaiki kepada Menteri atau pejabat yang ditunjuk."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/112> .

<http://example.org/legal/document/UU13-2003/pasal/113/ayat/1> a legal:Ayat ;
    legal:hasContent "Perubahan peraturan perusahaan sebelum berakhir jangka waktu berlakunya hanya dapat dilakukan atas dasar kesepakatan antara pengusaha dan wakil pekerja/buruh."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/113> .

<http://example.org/legal/document/UU13-2003/pasal/113/ayat/2> a legal:Ayat ;
    legal:hasContent "Peraturan perusahaan hasil perubahan sebagaimana dimaksud dalam ayat (1) harus mendapat pengesa-han dari Menteri atau pejabat yang ditunjuk."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/113> .

<http://example.org/legal/document/UU13-2003/pasal/114> a legal:Pasal ;
    legal:hasContent "Pengusaha wajib memberitahukan dan menjelaskan isi serta memberikan naskah peraturan perusahaan atau perubahannya kepada pekerja/buruh."^^xsd:string ;
    legal:hasKey 114 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/6> .

<http://example.org/legal/document/UU13-2003/pasal/115> a legal:Pasal ;
    legal:hasContent "Ketentuan mengenai tata cara pembuatan dan pengesahan peraturan perusahaan diatur dengan Keputusan Menteri."^^xsd:string ;
    legal:hasKey 115 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/6> .

<http://example.org/legal/document/UU13-2003/pasal/116/ayat/1> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja bersama dibuat oleh serikat pekerja/serikat buruh atau beberapa serikat pekerja/serikat buruh yang telah tercatat pada instansi yang bertanggung jawab di bidang ketenagakerjaan dengan pengusaha atau beberapa pengusaha."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/116> .

<http://example.org/legal/document/UU13-2003/pasal/116/ayat/2> a legal:Ayat ;
    legal:hasContent "Penyusunan perjanjian kerja bersama sebagaimana dimaksud dalam ayat (1) dilaksanakan secara musya-warah."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/116> .

<http://example.org/legal/document/UU13-2003/pasal/116/ayat/3> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja bersama sebagaimana dimaksud dalam ayat (1) harus dibuat secara tertulis dengan huruf latin dan menggunakan bahasa Indonesia."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/116> .

<http://example.org/legal/document/UU13-2003/pasal/116/ayat/4> a legal:Ayat ;
    legal:hasContent "Dalam hal terdapat perjanjian kerja bersama yang dibuat tidak menggunakan bahasa Indonesia, maka per-janjian kerja bersama tersebut harus diterjemahkan dalam bahasa Indonesia oleh penerjemah tersumpah dan terjemahan tersebut dianggap sudah memenuhi ketentuan sebagaimana dimaksud dalam ayat (3)."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/116> .

<http://example.org/legal/document/UU13-2003/pasal/117> a legal:Pasal ;
    legal:hasContent "Dalam hal musyawarah sebagaimana dimaksud dalam Pasal 116 ayat (2) tidak mencapai kesepakatan, maka penyelesaiannya dilakukan melalui prosedur penyelesaian perselisihan hubungan industrial."^^xsd:string ;
    legal:hasKey 117 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/7> .

<http://example.org/legal/document/UU13-2003/pasal/118> a legal:Pasal ;
    legal:hasContent "Dalam 1 (satu) perusahaan hanya dapat dibuat 1 (satu) perjanjian kerja bersama yang berlaku bagi seluruh pekerja/buruh di perusahaan."^^xsd:string ;
    legal:hasKey 118 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/7> .

<http://example.org/legal/document/UU13-2003/pasal/119/ayat/1> a legal:Ayat ;
    legal:hasContent "Dalam hal di satu perusahaan hanya terdapat satu serikat pekerja/serikat buruh, maka serikat pekerja/seri-kat buruh tersebut berhak mewakili pekerja/buruh dalam perundingan pembuatan perjanjian kerja bersama dengan pengusaha apabila memiliki jumlah anggota lebih dari 50% (lima puluh perseratus) dari jumlah seluruh pekerja/buruh di perusahaan yang bersangkutan."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/119> .

<http://example.org/legal/document/UU13-2003/pasal/119/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam hal di satu perusahaan hanya terdapat satu serikat pekerja/serikat buruh sebagaimana dimaksud pada ayat (1) tetapi tidak memiliki jumlah anggota lebih dari 50% (lima puluh perseratus) dari jumlah seluruh pekerja/buruh di perusahaan maka serikat pekerja/serikat buruh dapat mewakili pekerja/buruh dalam perundingan dengan pengusaha apabila serikat pekerja/serikat buruh yang bersangkutan telah mendapat dukungan lebih 50% (lima puluh perseratus) dari jumlah seluruh pekerja/buruh di perusahaan melalui pemungutan suara."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/119> .

<http://example.org/legal/document/UU13-2003/pasal/119/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal dukungan sebagaimana dimaksud dalam ayat (2) tidak tercapai maka serikat pekerja/serikat buruh yang bersangkutan dapat mengajukan kembali permintaan untuk merundingkan perjanjian kerja bersama dengan pengusaha setelah melampaui jangka waktu 6 (enam) bulan terhitung sejak dilakukannya pemungutan suara dengan mengikuti prosedur sebagaimana dimaksud dalam ayat (2)."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/119> .

<http://example.org/legal/document/UU13-2003/pasal/12/ayat/1> a legal:Ayat ;
    legal:hasContent "Pengusaha bertanggung jawab atas peningkatan dan/atau pengembangan kompetensi pekerjanya melalui pelatihan kerja."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/12> .

<http://example.org/legal/document/UU13-2003/pasal/12/ayat/2> a legal:Ayat ;
    legal:hasContent "Peningkatan dan/atau pengembangan kompetensi sebagaimana dimaksud dalam ayat (1) diwajibkan bagi pengusaha yang memenuhi persyaratan yang diatur dengan Keputusan Menteri."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/12> .

<http://example.org/legal/document/UU13-2003/pasal/12/ayat/3> a legal:Ayat ;
    legal:hasContent "Setiap pekerja/buruh memiliki kesempatan yang sama untuk mengikuti pelatihan kerja sesuai dengan bi-dang tugasnya."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/12> .

<http://example.org/legal/document/UU13-2003/pasal/120/ayat/1> a legal:Ayat ;
    legal:hasContent "Dalam hal di satu perusahaan terdapat lebih dari 1 (satu) serikat pekerja/serikat buruh maka yang berhak mewakili pekerja/buruh melakukan perundingan dengan pengusaha yang jumlah keanggotaannya lebih dari 50% (lima puluh perseratus) dari seluruh jumlah pekerja/buruh di perusahaan tersebut."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/120> .

<http://example.org/legal/document/UU13-2003/pasal/120/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam hal ketentuan sebagaimana dimaksud dalam ayat (1) tidak terpenuhi, maka serikat pekerja/serikat buruh dapat melakukan koalisi sehingga tercapai jumlah lebih dari 50% (lima puluh perseratus) dari seluruh jumlah pekerja/buruh di perusahaan tersebut untuk mewakili dalam perundingan dengan pengusaha."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/120> .

<http://example.org/legal/document/UU13-2003/pasal/120/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal ketentuan sebagaimana dimaksud dalam ayat (1) atau ayat (2) tidak terpenuhi, maka para seri-kat pekerja/serikat buruh membentuk tim perunding yang keanggotaannya ditentukan secara proporsional berdasarkan jumlah anggota masing-masing serikat pekerja/serikat buruh."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/120> .

<http://example.org/legal/document/UU13-2003/pasal/121> a legal:Pasal ;
    legal:hasContent "Keanggotaan serikat pekerja/serikat buruh sebagaimana dimaksud dalam Pasal 119 dan Pasal 120 dibuktikan dengan kartu tanda anggota."^^xsd:string ;
    legal:hasKey 121 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/7> .

<http://example.org/legal/document/UU13-2003/pasal/122> a legal:Pasal ;
    legal:hasContent "Pemungutan suara sebagaimana dimaksud dalam Pasal 119 ayat (2) diselenggarakan oleh panitia yang terdiri dari wakil-wakil pekerja/buruh dan pengurus serikat pekerja/serikat buruh yang disaksikan oleh pihak pejabat yang bertanggung jawab di bidang ketenagakerjaan dan pengusaha."^^xsd:string ;
    legal:hasKey 122 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/7> .

<http://example.org/legal/document/UU13-2003/pasal/123/ayat/1> a legal:Ayat ;
    legal:hasContent "Masa berlakunya perjanjian kerja bersama paling lama 2 (dua) tahun."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/123> .

<http://example.org/legal/document/UU13-2003/pasal/123/ayat/2> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja bersama sebagaimana dimaksud dalam ayat (1) dapat diperpanjang masa berlakunya pa-ling lama 1 (satu) tahun berdasarkan kesepakatan tertulis antara pengusaha dengan serikat pekerja/serikat buruh."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/123> .

<http://example.org/legal/document/UU13-2003/pasal/123/ayat/3> a legal:Ayat ;
    legal:hasContent "Perundingan pembuatan perjanjian kerja bersama berikutnya dapat dimulai paling cepat 3 (tiga) bulan se-belum berakhirnya perjanjian kerja bersama yang sedang berlaku."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/123> .

<http://example.org/legal/document/UU13-2003/pasal/123/ayat/4> a legal:Ayat ;
    legal:hasContent "Dalam hal perundingan sebagaimana dimaksud dalam ayat (3) tidak mencapai kesepakatan maka perjan-jian kerja bersama yang sedang berlaku, tetap berlaku untuk paling lama 1 (satu) tahun."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/123> .

<http://example.org/legal/document/UU13-2003/pasal/124/ayat/1/point/a> a legal:Point ;
    legal:hasContent "hak dan kewajiban pengusaha;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/124/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/124/ayat/1/point/b> a legal:Point ;
    legal:hasContent "hak dan kewajiban serikat pekerja/serikat buruh serta pekerja/buruh;"^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/124/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/124/ayat/1/point/c> a legal:Point ;
    legal:hasContent "jangka waktu dan tanggal mulai berlakunya perjanjian kerja bersama; dan"^^xsd:string ;
    legal:hasKey "c"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/124/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/124/ayat/1/point/d> a legal:Point ;
    legal:hasContent "tanda tangan para pihak pembuat perjanjian kerja bersama."^^xsd:string ;
    legal:hasKey "d"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/124/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/124/ayat/2> a legal:Ayat ;
    legal:hasContent "Ketentuan dalam perjanjian kerja bersama tidak boleh bertentangan dengan peraturan perundang- undangan yang berlaku."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/124> .

<http://example.org/legal/document/UU13-2003/pasal/124/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal isi perjanjian kerja bersama bertentangan dengan peraturan perundang-undangan yang berlaku sebagaimana dimaksud dalam ayat (2), maka ketentuan yang bertentangan tersebut batal demi hukum dan yang berlaku adalah ketentuan dalam peraturan perundang-undangan."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/124> .

<http://example.org/legal/document/UU13-2003/pasal/125> a legal:Pasal ;
    legal:hasContent "Dalam hal kedua belah pihak sepakat mengadakan perubahan perjanjian kerja bersama, maka perubahan tersebut merupakan bagian yang tidak terpisahkan dari perjanjian kerja bersama yang sedang berlaku."^^xsd:string ;
    legal:hasKey 125 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/7> .

<http://example.org/legal/document/UU13-2003/pasal/126/ayat/1> a legal:Ayat ;
    legal:hasContent "Pengusaha, serikat pekerja/serikat buruh dan pekerja/buruh wajib melaksanakan ketentuan yang ada da-lam perjanjian kerja bersama."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/126> .

<http://example.org/legal/document/UU13-2003/pasal/126/ayat/2> a legal:Ayat ;
    legal:hasContent "Pengusaha dan serikat pekerja/serikat buruh wajib memberitahukan isi perjanjian kerja bersama atau peru-bahannya kepada seluruh pekerja/ buruh."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/126> .

<http://example.org/legal/document/UU13-2003/pasal/126/ayat/3> a legal:Ayat ;
    legal:hasContent "Pengusaha harus mencetak dan membagikan naskah perjanjian kerja bersama kepada setiap pekerja/ buruh atas biaya perusahaan."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/126> .

<http://example.org/legal/document/UU13-2003/pasal/127/ayat/1> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja yang dibuat oleh pengusaha dan pekerja/buruh tidak boleh bertentangan dengan perjanjian kerja bersama."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/127> .

<http://example.org/legal/document/UU13-2003/pasal/127/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam hal ketentuan dalam perjanjian kerja sebagaimana dimaksud dalam ayat (1) bertentangan dengan perjanjian kerja bersama, maka ketentuan dalam perjanjian kerja tersebut batal demi hukum dan yang berlaku adalah ketentuan dalam perjanjian kerja bersama."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/127> .

<http://example.org/legal/document/UU13-2003/pasal/128> a legal:Pasal ;
    legal:hasContent "Dalam hal perjanjian kerja tidak memuat aturan-aturan yang diatur dalam perjanjian kerja bersama maka yang berlaku adalah aturan-aturan dalam perjanjian kerja bersama."^^xsd:string ;
    legal:hasKey 128 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/7> .

<http://example.org/legal/document/UU13-2003/pasal/129/ayat/1> a legal:Ayat ;
    legal:hasContent "Pengusaha dilarang mengganti perjanjian kerja bersama dengan peraturan perusahaan, selama di perusa-haan yang bersangkutan masih ada serikat pekerja/serikat buruh."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/129> .

<http://example.org/legal/document/UU13-2003/pasal/129/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam hal di perusahaan tidak ada lagi serikat pekerja/serikat buruh dan perjanjian kerja bersama diganti dengan peraturan perusahaan, maka ketentuan yang ada dalam peraturan perusahaan tidak boleh lebih rendah dari ketentuan yang ada dalam perjanjian kerja bersama."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/129> .

<http://example.org/legal/document/UU13-2003/pasal/13/ayat/1> a legal:Ayat ;
    legal:hasContent "Pelatihan kerja diselenggarakan oleh lembaga pelatihan kerja pemerintah dan/atau lembaga pelatihan kerja swasta."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/13> .

<http://example.org/legal/document/UU13-2003/pasal/13/ayat/2> a legal:Ayat ;
    legal:hasContent "Pelatihan kerja dapat diselenggarakan di tempat pelatihan atau tempat kerja."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/13> .

<http://example.org/legal/document/UU13-2003/pasal/13/ayat/3> a legal:Ayat ;
    legal:hasContent "Lembaga pelatihan kerja pemerintah sebagaimana dimaksud dalam ayat (1) dalam menyelenggarakan pe-latihan kerja dapat bekerja sama dengan swasta."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/13> .

<http://example.org/legal/document/UU13-2003/pasal/130/ayat/1> a legal:Ayat ;
    legal:hasContent "Dalam hal perjanjian kerja bersama yang sudah berakhir masa berlakunya akan diperpanjang atau diper-baharui dan di perusahaan tersebut hanya terdapat 1 (satu) serikat pekerja/serikat buruh, maka perpanjangan atau pembuatan pembaharuan perjanjian kerja bersama tidak mensyaratkan ketentuan dalam Pasal 119."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/130> .

<http://example.org/legal/document/UU13-2003/pasal/130/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam hal perjanjian kerja bersama yang sudah berakhir masa berlakunya akan diperpanjang atau diper-baharui dan di perusahaan tersebut terdapat lebih dari 1 (satu) serikat pekerja/serikat buruh dan serikat pekerja/serikat buruh yang dulu berunding tidak lagi memenuhi ketentuan Pasal 120 ayat (1), maka perpanjangan atau pembuatan pembaharuan perjanjian kerja bersama dilakukan oleh serikat pekerja/serikat buruh yang anggotanya lebih 50% (lima puluh perseratus) dari jumlah seluruh pekerja/buruh di perusahaan bersama-sama dengan serikat pekerja/serikat buruh yang membuat perjanjian kerja bersama terdahulu dengan membentuk tim perunding secara proporsional."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/130> .

<http://example.org/legal/document/UU13-2003/pasal/130/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal perjanjian kerja bersama yang sudah berakhir masa berlakunya akan diperpanjang atau diper-baharui dan di perusahaan tersebut terdapat lebih dari 1 (satu) serikat pekerja/ serikat buruh dan tidak satupun serikat pekerja/serikat buruh yang ada memenuhi ketentuan Pasal 120 ayat (1), maka perpanjangan atau pembuatan pembaharuan perjanjian kerja bersama dilakukan menurut ketentuan Pasal 120 ayat (2) dan ayat (3)."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/130> .

<http://example.org/legal/document/UU13-2003/pasal/131/ayat/1> a legal:Ayat ;
    legal:hasContent "Dalam hal terjadi pembubaran serikat pekerja/serikat buruh atau pengalihan kepemilikan perusahaan maka perjanjian kerja bersama tetap berlaku sampai berakhirnya jangka waktu perjanjian kerja bersama."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/131> .

<http://example.org/legal/document/UU13-2003/pasal/131/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam hal terjadi penggabungan perusahaan (merger) dan masing-masing perusahaan mempunyai perjan-jian kerja bersama maka perjanjian kerja bersama yang berlaku adalah perjanjian kerja bersama yang lebih menguntungkan pekerja/buruh."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/131> .

<http://example.org/legal/document/UU13-2003/pasal/131/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal terjadi penggabungan perusahaan (merger) antara perusahaan yang mempunyai perjanjian kerja bersama dengan perusahaan yang belum mempunyai perjanjian kerja bersama maka perjanjian kerja bersama tersebut berlaku bagi perusahaan yang bergabung (merger) sampai dengan berakhirnya jangka waktu perjanjian kerja bersama."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/131> .

<http://example.org/legal/document/UU13-2003/pasal/132/ayat/1> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja bersama mulai berlaku pada hari penandatanganan kecuali ditentukan lain dalam perjanjian kerja bersama tersebut."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/132> .

<http://example.org/legal/document/UU13-2003/pasal/132/ayat/2> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja bersama yang ditandatangani oleh pihak yang membuat perjanjian kerja bersama selan-jutnya didaftarkan oleh pengusaha pada instansi yang bertanggung jawab di bidang ketenagakerjaan."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/132> .

<http://example.org/legal/document/UU13-2003/pasal/133> a legal:Pasal ;
    legal:hasContent "Ketentuan mengenai persyaratan serta tata cara pembuatan, perpanjangan, perubahan, dan pendaftaran perjanjian kerja bersama diatur dengan Keputusan Menteri."^^xsd:string ;
    legal:hasKey 133 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/7> .

<http://example.org/legal/document/UU13-2003/pasal/134> a legal:Pasal ;
    legal:hasContent "Dalam mewujudkan pelaksanaan hak dan kewajiban pekerja/buruh dan pengusaha, pemerintah wajib melaksanakan pengawasan dan penegakan peraturan perundang-undangan ketenagakerjaan."^^xsd:string ;
    legal:hasKey 134 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/7> .

<http://example.org/legal/document/UU13-2003/pasal/135> a legal:Pasal ;
    legal:hasContent "Pelaksanaan peraturan perundang-undangan ketenagakerjaan dalam mewujudkan hubungan industrial merupakan tanggung jawab pekerja/buruh, pengusaha, dan pemerintah."^^xsd:string ;
    legal:hasKey 135 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/7> .

<http://example.org/legal/document/UU13-2003/pasal/136/ayat/1> a legal:Ayat ;
    legal:hasContent "Penyelesaian perselisihan hubungan industrial wajib dilaksanakan oleh pengusaha dan pekerja/buruh atau serikat pekerja/serikat buruh secara musyawarah untuk mufakat."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/136> .

<http://example.org/legal/document/UU13-2003/pasal/136/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam hal penyelesaian secara musyawarah untuk mufakat sebagaimana dimaksud dalam ayat (1) tidak tercapai, maka pengusaha dan pekerja/ buruh atau serikat pekerja/serikat buruh menyelesaikan perselisihan hubungan industrial melalui prosedur penyelesaian perselisihan hubungan industrial yang diatur dengan undang-undang."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/136> .

<http://example.org/legal/document/UU13-2003/pasal/137> a legal:Pasal ;
    legal:hasContent "Mogok kerja sebagai hak dasar pekerja/buruh dan serikat pekerja/serikat buruh dilakukan secara sah, tertib, dan damai sebagai akibat gagalnya perundingan."^^xsd:string ;
    legal:hasKey 137 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/8/paragraf/2> .

<http://example.org/legal/document/UU13-2003/pasal/138/ayat/1> a legal:Ayat ;
    legal:hasContent "Pekerja/buruh dan/atau serikat pekerja/serikat buruh yang bermaksud mengajak pekerja/buruh lain untuk mogok kerja pada saat mogok kerja berlangsung dilakukan dengan tidak melanggar hukum."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/138> .

<http://example.org/legal/document/UU13-2003/pasal/138/ayat/2> a legal:Ayat ;
    legal:hasContent "Pekerja/buruh yang diajak mogok kerja sebagaimana dimaksud dalam ayat (1), dapat memenuhi atau tidak memenuhi ajakan tersebut."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/138> .

<http://example.org/legal/document/UU13-2003/pasal/139> a legal:Pasal ;
    legal:hasContent "Pelaksanaan mogok kerja bagi pekerja/buruh yang bekerja pada perusahaan yang melayani kepentingan umum dan/atau perusahaan yang jenis kegiatan-nya membahayakan keselamatan jiwa manusia diatur sedemikian rupa sehingga tidak mengganggu kepentingan umum dan/atau membahayakan keselamatan orang lain."^^xsd:string ;
    legal:hasKey 139 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/8/paragraf/2> .

<http://example.org/legal/document/UU13-2003/pasal/14/ayat/1> a legal:Ayat ;
    legal:hasContent "Lembaga pelatihan kerja swasta dapat berbentuk badan hukum Indonesia atau perorangan."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/14> .

<http://example.org/legal/document/UU13-2003/pasal/14/ayat/2> a legal:Ayat ;
    legal:hasContent "Lembaga pelatihan kerja swasta sebagaimana dimaksud dalam ayat (1) wajib memperoleh izin atau men daftar ke instansi yang bertanggung jawab di bidang ketenagakerjaan di kabupaten/kota."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/14> .

<http://example.org/legal/document/UU13-2003/pasal/14/ayat/3> a legal:Ayat ;
    legal:hasContent "Lembaga pelatihan kerja yang diselenggarakan oleh instansi pemerintah mendaftarkan kegiatannya kepada instansi yang bertanggung jawab di bidang ketenagakerjaan di kabupaten/kota."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/14> .

<http://example.org/legal/document/UU13-2003/pasal/14/ayat/4> a legal:Ayat ;
    legal:hasContent "Ketentuan mengenai tata cara perizinan dan pendaftaran lembaga pelatihan kerja sebagaimana dimaksud dalam ayat (2) dan ayat (3) diatur dengan Keputusan Menteri."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/14> .

<http://example.org/legal/document/UU13-2003/pasal/140/ayat/1> a legal:Ayat ;
    legal:hasContent "Sekurang-kurangnya dalam waktu 7 (tujuh) hari kerja sebelum mogok kerja dilaksanakan, pekerja/buruh dan serikat pekerja/serikat buruh wajib memberitahukan secara tertulis kepada pengusaha dan instansi yang bertanggung jawab di bidang ketenagakerjaan setempat."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/140> .

<http://example.org/legal/document/UU13-2003/pasal/140/ayat/2/point/a> a legal:Point ;
    legal:hasContent "waktu (hari, tanggal, dan jam) dimulai dan diakhiri mogok kerja;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/140/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/140/ayat/2/point/b> a legal:Point ;
    legal:hasContent "tempat mogok kerja;"^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/140/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/140/ayat/2/point/c> a legal:Point ;
    legal:hasContent "alasan dan sebab-sebab mengapa harus melakukan mogok kerja; dan"^^xsd:string ;
    legal:hasKey "c"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/140/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/140/ayat/2/point/d> a legal:Point ;
    legal:hasContent "tanda tangan ketua dan sekretaris dan/atau masing-masing ketua dan sekretaris serikat pekerja/serikat buruh sebagai penanggung jawab mogok kerja."^^xsd:string ;
    legal:hasKey "d"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/140/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/140/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal mogok kerja akan dilakukan oleh pekerja/buruh yang tidak menjadi anggota serikat pekerja/ serikat buruh, maka pemberitahuan sebagaimana dimaksud dalam ayat (2) ditandatangani oleh perwakilan pekerja/buruh yang ditunjuk sebagai koordinator dan/atau penanggung jawab mogok kerja."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/140> .

<http://example.org/legal/document/UU13-2003/pasal/140/ayat/4/point/a> a legal:Point ;
    legal:hasContent "melarang para pekerja/buruh yang mogok kerja berada di lokasi kegiatan proses produksi; atau"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/140/ayat/4> .

<http://example.org/legal/document/UU13-2003/pasal/140/ayat/4/point/b> a legal:Point ;
    legal:hasContent "bila dianggap perlu melarang pekerja/buruh yang mogok kerja berada di lokasi perusahaan."^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/140/ayat/4> .

<http://example.org/legal/document/UU13-2003/pasal/141/ayat/1> a legal:Ayat ;
    legal:hasContent "Instansi pemerintah dan pihak perusahaan yang menerima surat pemberitahuan mogok kerja sebagaimana dimaksud dalam Pasal 140 wajib memberikan tanda terima."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/141> .

<http://example.org/legal/document/UU13-2003/pasal/141/ayat/2> a legal:Ayat ;
    legal:hasContent "Sebelum dan selama mogok kerja berlangsung, instansi yang bertanggung jawab di bidang ketenagakerjaan wajib menyelesaikan masalah yang menyebabkan timbulnya pemogokan dengan mempertemukan dan merundingkannya dengan para pihak yang berselisih."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/141> .

<http://example.org/legal/document/UU13-2003/pasal/141/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal perundingan sebagaimana dimaksud dalam ayat (2) menghasilkan kesepakatan, maka harus dibuatkan perjanjian bersama yang ditandatangani oleh para pihak dan pegawai dari instansi yang bertanggung jawab di bidang ketenagakerjaan sebagai saksi."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/141> .

<http://example.org/legal/document/UU13-2003/pasal/141/ayat/4> a legal:Ayat ;
    legal:hasContent "Dalam hal perundingan sebagaimana dimaksud dalam ayat (2) tidak menghasilkan kesepakatan, maka pegawai dari instansi yang bertanggung jawab di bidang ketenagakerjaan segera menyerahkan masalah yang menyebabkan terjadinya mogok kerja kepada lembaga penyelesaian perselisihan hubungan industrial yang berwenang."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/141> .

<http://example.org/legal/document/UU13-2003/pasal/141/ayat/5> a legal:Ayat ;
    legal:hasContent "Dalam hal perundingan tidak menghasilkan kesepakatan sebagaimana dimaksud dalam ayat (4), maka atas dasar perundingan antara pengusaha dengan serikat pekerja/serikat buruh atau penanggung jawab mogok kerja, mogok kerja dapat diteruskan atau dihentikan untuk sementara atau dihentikan sama sekali."^^xsd:string ;
    legal:hasKey 5 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/141> .

<http://example.org/legal/document/UU13-2003/pasal/142/ayat/1> a legal:Ayat ;
    legal:hasContent "Mogok kerja yang dilakukan tidak memenuhi ketentuan sebagaimana dimaksud dalam Pasal 139 dan Pa-sal 140 adalah mogok kerja tidak sah."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/142> .

<http://example.org/legal/document/UU13-2003/pasal/142/ayat/2> a legal:Ayat ;
    legal:hasContent "Akibat hukum dari mogok kerja yang tidak sah sebagaimana dimaksud dalam ayat (1) akan diatur dengan Keputusan Menteri."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/142> .

<http://example.org/legal/document/UU13-2003/pasal/143/ayat/1> a legal:Ayat ;
    legal:hasContent "Siapapun tidak dapat menghalang-halangi pekerja/buruh dan serikat pekerja/serikat buruh untuk mengguna kan hak mogok kerja yang dilakukan secara sah, tertib, dan damai."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/143> .

<http://example.org/legal/document/UU13-2003/pasal/143/ayat/2> a legal:Ayat ;
    legal:hasContent "Siapapun dilarang melakukan penangkapan dan/atau penahanan terhadap pekerja/buruh dan pengurus serikat pekerja/serikat buruh yang melakukan mogok kerja secara sah, tertib, dan damai sesuai dengan peraturan perundang-undangan yang berlaku."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/143> .

<http://example.org/legal/document/UU13-2003/pasal/144/point/a> a legal:Point ;
    legal:hasContent "mengganti pekerja/buruh yang mogok kerja dengan pekerja/buruh lain dari luar perusahaan; atau"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/144> .

<http://example.org/legal/document/UU13-2003/pasal/144/point/b> a legal:Point ;
    legal:hasContent "memberikan sanksi atau tindakan balasan dalam bentuk apapun kepada pekerja/buruh dan pengurus serikat pekerja/serikat buruh selama dan sesudah melakukan mogok kerja."^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/144> .

<http://example.org/legal/document/UU13-2003/pasal/145> a legal:Pasal ;
    legal:hasContent "Dalam hal pekerja/buruh yang melakukan mogok kerja secara sah dalam melakukan tuntutan hak normatif yang sungguh-sungguh dilanggar oleh pengusaha, pekerja/buruh berhak mendapatkan upah."^^xsd:string ;
    legal:hasKey 145 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/8/paragraf/2> .

<http://example.org/legal/document/UU13-2003/pasal/146/ayat/1> a legal:Ayat ;
    legal:hasContent "Penutupan perusahaan (lock out) merupakan hak dasar pengusaha untuk menolak pekerja/buruh sebagian atau seluruhnya untuk menjalankan pekerjaan sebagai akibat gagalnya perundingan."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/146> .

<http://example.org/legal/document/UU13-2003/pasal/146/ayat/2> a legal:Ayat ;
    legal:hasContent "Pengusaha tidak dibenarkan melakukan penutupan perusahaan (lock out) sebagai tindakan balasan sehubungan adanya tuntutan normatif dari pekerja/buruh dan/atau serikat pekerja/serikat buruh."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/146> .

<http://example.org/legal/document/UU13-2003/pasal/146/ayat/3> a legal:Ayat ;
    legal:hasContent "Tindakan penutupan perusahaan (lock out) harus dilakukan sesuai dengan ketentuan hukum yang berlaku."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/146> .

<http://example.org/legal/document/UU13-2003/pasal/147> a legal:Pasal ;
    legal:hasContent "Penutupan perusahaan (lock out) dilarang dilakukan pada perusahaan-perusahaan yang melayani kepentingan umum dan/atau jenis kegiatan yang membahayakan keselamatan jiwa manusia, meliputi rumah sakit, pelayanan jaringan air bersih, pusat pengendali telekomunikasi, pusat penyedia tenaga listrik, pengolahan minyak dan gas bumi, serta kereta api."^^xsd:string ;
    legal:hasKey 147 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/11/bagian/8/paragraf/3> .

<http://example.org/legal/document/UU13-2003/pasal/148/ayat/1> a legal:Ayat ;
    legal:hasContent "Pengusaha wajib memberitahukan secara tertulis kepada pekerja/buruh dan/atau serikat pekerja/serikat buruh, serta instansi yang bertanggung jawab di bidang ketenagakerjaan setempat sekurang-kurangnya 7 (tujuh) hari kerja sebelum penutupan perusahaan (lock out) dilaksanakan."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/148> .

<http://example.org/legal/document/UU13-2003/pasal/148/ayat/2/point/a> a legal:Point ;
    legal:hasContent "waktu (hari, tanggal, dan jam) dimulai dan diakhiri penutupan perusahaan (lock out); dan"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/148/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/148/ayat/2/point/b> a legal:Point ;
    legal:hasContent "alasan dan sebab-sebab melakukan penutupan perusahaan (lock out)."^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/148/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/148/ayat/3> a legal:Ayat ;
    legal:hasContent "Pemberitahuan sebagaimana dimaksud dalam ayat (1) ditandatangani oleh pengusaha dan/atau pimpinan perusahaan yang bersangkutan."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/148> .

<http://example.org/legal/document/UU13-2003/pasal/149/ayat/1> a legal:Ayat ;
    legal:hasContent "Pekerja/buruh atau serikat pekerja/serikat buruh dan instansi yang bertanggung jawab di bidang ketenaga-kerjaan yang menerima secara langsung surat pemberitahuan penutupan perusahaan (lock out) sebagaimana dimaksud dalam Pasal 148 harus memberikan tanda bukti penerimaan dengan mencantumkan hari, tanggal, dan jam penerimaan."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/149> .

<http://example.org/legal/document/UU13-2003/pasal/149/ayat/2> a legal:Ayat ;
    legal:hasContent "Sebelum dan selama penutupan perusahaan (lock out) berlangsung, instansi yang bertanggung jawab di bidang ketenagakerjaan berwenang langsung menyelesaikan masalah yang menyebabkan timbulnya penutupan perusahaan (lock out) dengan mempertemukan dan merundingkannya dengan para pihak yang berselisih."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/149> .

<http://example.org/legal/document/UU13-2003/pasal/149/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal perundingan sebagaimana dimaksud dalam ayat (2) menghasilkan kesepakatan, maka harus dibuat perjanjian bersama yang ditandatangani oleh para pihak dan pegawai dari instansi yang bertanggung jawab di bidang ketenagakerjaan sebagai saksi."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/149> .

<http://example.org/legal/document/UU13-2003/pasal/149/ayat/4> a legal:Ayat ;
    legal:hasContent "Dalam hal perundingan sebagaimana dimaksud dalam ayat (2) tidak menghasilkan kesepakatan, maka pegawai dari instansi yang bertanggung jawab di bidang ketenagakerjaan segera menyerahkan masalah yang menyebabkan terjadinya penutupan perusahaan (lock out) kepada lembaga penyelesaian perselisihan hubungan industrial."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/149> .

<http://example.org/legal/document/UU13-2003/pasal/149/ayat/5> a legal:Ayat ;
    legal:hasContent "Apabila perundingan tidak menghasilkan kesepakatan sebagaimana dimaksud dalam ayat (4), maka atas dasar perundingan antara pengusaha dan serikat pekerja/serikat buruh, penutupan perusahaan (lock out) dapat diteruskan atau dihentikan untuk sementara atau dihentikan sama sekali."^^xsd:string ;
    legal:hasKey 5 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/149> .

<http://example.org/legal/document/UU13-2003/pasal/149/ayat/6/point/a> a legal:Point ;
    legal:hasContent "pekerja/buruh atau serikat pekerja/serikat buruh melanggar prosedur mogok kerja sebagaimana dimaksud dalam Pasal 140;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/149/ayat/6> .

<http://example.org/legal/document/UU13-2003/pasal/149/ayat/6/point/b> a legal:Point ;
    legal:hasContent "pekerja/buruh atau serikat pekerja/serikat buruh melanggar ketentuan normatif yang ditentukan dalam perjanjian kerja, peraturan perusahaan, perjanjian kerja bersama, atau peraturan perundang-undangan yang berlaku."^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/149/ayat/6> .

<http://example.org/legal/document/UU13-2003/pasal/15/point/a> a legal:Point ;
    legal:hasContent "tersedianya tenaga kepelatihan;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/15> .

<http://example.org/legal/document/UU13-2003/pasal/15/point/b> a legal:Point ;
    legal:hasContent "adanya kurikulum yang sesuai dengan tingkat pelatihan;"^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/15> .

<http://example.org/legal/document/UU13-2003/pasal/15/point/c> a legal:Point ;
    legal:hasContent "tersedianya sarana dan prasarana pelatihan kerja; dan"^^xsd:string ;
    legal:hasKey "c"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/15> .

<http://example.org/legal/document/UU13-2003/pasal/15/point/d> a legal:Point ;
    legal:hasContent "tersedianya dana bagi kelangsungan kegiatan penyelenggaraan pelatihan kerja."^^xsd:string ;
    legal:hasKey "d"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/15> .

<http://example.org/legal/document/UU13-2003/pasal/150> a legal:Pasal ;
    legal:hasContent "Ketentuan mengenai pemutusan hubungan kerja dalam undang-undang ini meliputi pemutusan hubungan kerja yang terjadi di badan usaha yang berbadan hukum atau tidak, milik orang perseorangan, milik persekutuan atau milik badan hukum, baik milik swasta maupun milik negara, maupun usaha-usaha sosial dan usaha-usaha lain yang mempunyai pengurus dan mempekerjakan orang lain dengan membayar upah atau imbalan dalam bentuk lain."^^xsd:string ;
    legal:hasKey 150 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/12> .

<http://example.org/legal/document/UU13-2003/pasal/151/ayat/1> a legal:Ayat ;
    legal:hasContent "Pengusaha, pekerja/buruh, serikat pekerja/serikat buruh, dan pemerintah, dengan segala upaya harus mengusahakan agar jangan terjadi pemutusan hubungan kerja."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/151> .

<http://example.org/legal/document/UU13-2003/pasal/151/ayat/2> a legal:Ayat ;
    legal:hasContent "Dalam hal segala upaya telah dilakukan, tetapi pemutusan hubungan kerja tidak dapat dihindari, maka maksud pemutusan hubungan kerja wajib dirundingkan oleh pengusaha dan serikat pekerja/serikat buruh atau dengan pekerja/buruh apabila pekerja/buruh yang bersangkutan tidak menjadi anggota serikat pekerja/serikat buruh."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/151> .

<http://example.org/legal/document/UU13-2003/pasal/151/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal perundingan sebagaimana dimaksud dalam ayat (2) benar-benar tidak menghasilkan persetu-juan, pengusaha hanya dapat memutuskan hubungan kerja dengan pekerja/buruh setelah memperoleh penetapan dari lembaga penyelesaian perselisihan hubungan industrial."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/151> .

<http://example.org/legal/document/UU13-2003/pasal/152/ayat/1> a legal:Ayat ;
    legal:hasContent "Permohonan penetapan pemutusan hubungan kerja diajukan secara tertulis kepada lembaga penyelesaian perselisihan hubungan industrial disertai alasan yang menjadi dasarnya."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/152> .

<http://example.org/legal/document/UU13-2003/pasal/152/ayat/2> a legal:Ayat ;
    legal:hasContent "Permohonan penetapan sebagaimana dimaksud dalam ayat (1) dapat diterima oleh lembaga penyelesaian perselisihan hubungan industrial apabila telah dirundangkan sebagaimana dimaksud dalam Pasal 151 ayat (2)."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/152> .

<http://example.org/legal/document/UU13-2003/pasal/152/ayat/3> a legal:Ayat ;
    legal:hasContent "Penetapan atas permohonan pemutusan hubungan kerja hanya dapat diberikan oleh lembaga penyelesaian perselisihan hubungan industrial jika ternyata maksud untuk memutuskan hubungan kerja telah dirundingkan, tetapi perundingan tersebut tidak menghasilkan kesepakatan."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/152> .

<http://example.org/legal/document/UU13-2003/pasal/153/ayat/1/point/a> a legal:Point ;
    legal:hasContent "pekerja/buruh berhalangan masuk kerja karena sakit menurut keterangan dokter selama waktu tidak melampaui 12 (dua belas) bulan secara terus-menerus;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/153/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/153/ayat/1/point/b> a legal:Point ;
    legal:hasContent "pekerja/buruh berhalangan menjalankan pekerjaannya karena memenuhi kewajiban terhadap negara sesuai dengan ketentuan peraturan perundang-undangan yang berlaku;"^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/153/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/153/ayat/1/point/c> a legal:Point ;
    legal:hasContent "pekerja/buruh menjalankan ibadah yang diperintahkan agamanya;"^^xsd:string ;
    legal:hasKey "c"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/153/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/153/ayat/1/point/d> a legal:Point ;
    legal:hasContent "pekerja/buruh menikah;"^^xsd:string ;
