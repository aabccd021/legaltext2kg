    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/182/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/182/ayat/2/point/b> a legal:Point ;
