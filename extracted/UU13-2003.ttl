    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/43> .

<http://example.org/legal/document/UU13-2003/pasal/43/ayat/2/point/a> a legal:Point ;
    legal:hasContent "alasan penggunaan tenaga kerja asing;"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/43/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/43/ayat/2/point/b> a legal:Point ;
    legal:hasContent "jabatan dan/atau kedudukan tenaga kerja asing dalam struktur organisasi perusahaan yang bersangkutan;"^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/43/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/43/ayat/2/point/c> a legal:Point ;
    legal:hasContent "jangka waktu penggunaan tenaga kerja asing; dan"^^xsd:string ;
    legal:hasKey "c"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/43/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/43/ayat/2/point/d> a legal:Point ;
    legal:hasContent "penunjukan tenaga kerja warga negara Indonesia sebagai pendamping tenaga kerja asing yang dipekerjakan."^^xsd:string ;
    legal:hasKey "d"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/43/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/43/ayat/3> a legal:Ayat ;
    legal:hasContent "Ketentuan sebagaimana dimaksud dalam ayat (1) tidak berlaku bagi instansi pemerintah, badan- badan internasional dan perwakilan negara asing."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/43> .

<http://example.org/legal/document/UU13-2003/pasal/43/ayat/4> a legal:Ayat ;
    legal:hasContent "Ketentuan mengenai tata cara pengesahan rencana penggunaan tenaga kerja asing diatur dengan Keputu san Menteri."^^xsd:string ;
    legal:hasKey 4 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/43> .

<http://example.org/legal/document/UU13-2003/pasal/44/ayat/1> a legal:Ayat ;
    legal:hasContent "Pemberi kerja tenaga kerja asing wajib menaati ketentuan mengenai jabatan dan standar kompetensi yang berlaku."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/44> .

<http://example.org/legal/document/UU13-2003/pasal/44/ayat/2> a legal:Ayat ;
    legal:hasContent "Ketentuan mengenai jabatan dan standar kompetensi sebagaimana dimaksud dalam ayat (1) diatur dengan Keputusan Menteri."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/44> .

<http://example.org/legal/document/UU13-2003/pasal/45/ayat/1/point/a> a legal:Point ;
    legal:hasContent "menunjuk tenaga kerja warga negara Indonesia sebagai tenaga pendamping tenaga kerja asing yang dipekerjakan untuk alih teknologi dan alih keahlian dari tenaga kerja asing; dan"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/45/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/45/ayat/1/point/b> a legal:Point ;
    legal:hasContent "melaksanakan pendidikan dan pelatihan kerja bagi tenaga kerja Indonesia sebagaimana dimaksud pada huruf a yang sesuai dengan kualifikasi jabatan yang diduduki oleh tenaga kerja asing."^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/45/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/45/ayat/2> a legal:Ayat ;
    legal:hasContent "Ketentuan sebagaimana dimaksud dalam ayat (1) tidak berlaku bagi tenaga kerja asing yang menduduki ja batan direksi dan/atau komisaris."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/45> .

<http://example.org/legal/document/UU13-2003/pasal/46/ayat/1> a legal:Ayat ;
