    legal:hasKey "g"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/54/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/54/ayat/1/point/h> a legal:Point ;
    legal:hasContent "tempat dan tanggal perjanjian kerja dibuat; dan"^^xsd:string ;
    legal:hasKey "h"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/54/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/54/ayat/1/point/i> a legal:Point ;
    legal:hasContent "tanda tangan para pihak dalam perjanjian kerja."^^xsd:string ;
    legal:hasKey "i"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/54/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/54/ayat/2> a legal:Ayat ;
    legal:hasContent "Ketentuan dalam perjanjian kerja sebagaimana dimaksud dalam ayat (1) huruf e dan f, tidak boleh ber-tentangan dengan peraturan perusahaan, perjanjian kerja bersama, dan peraturan perundang undangan yang berlaku."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/54> .

<http://example.org/legal/document/UU13-2003/pasal/54/ayat/3> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja sebagaimana dimaksud dalam ayat (1) dibuat sekurang kurangnya rangkap 2 (dua), yang mempunyai kekuatan hukum yang sama, serta pekerja/buruh dan pengusaha masing masing mendapat 1 (satu) perjanjian kerja."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/54> .

<http://example.org/legal/document/UU13-2003/pasal/55> a legal:Pasal ;
    legal:hasContent "Perjanjian kerja tidak dapat ditarik kembali dan/atau diubah, kecuali atas persetujuan para pihak."^^xsd:string ;
    legal:hasKey 55 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/9> .

<http://example.org/legal/document/UU13-2003/pasal/56/ayat/1> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja dibuat untuk waktu tertentu atau untuk waktu tidak tertentu."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/56> .

<http://example.org/legal/document/UU13-2003/pasal/56/ayat/2/point/a> a legal:Point ;
    legal:hasContent "jangka waktu; atau"^^xsd:string ;
    legal:hasKey "a"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/56/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/56/ayat/2/point/b> a legal:Point ;
    legal:hasContent "selesainya suatu pekerjaan tertentu."^^xsd:string ;
    legal:hasKey "b"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/56/ayat/2> .

<http://example.org/legal/document/UU13-2003/pasal/57/ayat/1> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja untuk waktu tertentu dibuat secara tertulis serta harus menggunakan bahasa Indonesia dan huruf latin."^^xsd:string ;
    legal:hasKey 1 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/57> .

<http://example.org/legal/document/UU13-2003/pasal/57/ayat/2> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja untuk waktu tertentu yang dibuat tidak tertulis bertentangan dengan ketentuan sebagai mana dimaksud dalam ayat (1) dinyatakan sebagai perjanjian kerja untuk waktu tidak tertentu."^^xsd:string ;
    legal:hasKey 2 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/57> .

<http://example.org/legal/document/UU13-2003/pasal/57/ayat/3> a legal:Ayat ;
    legal:hasContent "Dalam hal perjanjian kerja dibuat dalam bahasa Indonesia dan bahasa asing, apabila kemudian terdapat perbedaan penafsiran antara keduanya, maka yang berlaku perjanjian kerja yang dibuat dalam bahasa Indonesia."^^xsd:string ;
    legal:hasKey 3 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/57> .

<http://example.org/legal/document/UU13-2003/pasal/58/ayat/1> a legal:Ayat ;
    legal:hasContent "Perjanjian kerja untuk waktu tertentu tidak dapat mensyaratkan adanya masa percobaan kerja."^^xsd:string ;
    legal:hasKey 1 ;
