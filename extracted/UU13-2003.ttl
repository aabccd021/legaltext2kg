    legal:hasKey "f"^^xsd:string ;
    legal:partOf <http://example.org/legal/document/UU13-2003/pasal/54/ayat/1> .

<http://example.org/legal/document/UU13-2003/pasal/54/ayat/1/point/g> a legal:Point ;
