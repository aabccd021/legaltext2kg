    legal:hasKey 49 ;
    legal:partOf <http://example.org/legal/document/UU13-2003/bab/8> .

<http://example.org/legal/document/UU13-2003/pasal/5> a legal:Pasal ;
    legal:hasContent "Setiap tenaga kerja memiliki kesempatan yang sama tanpa diskriminasi untuk memperoleh pekerjaan."^^xsd:string ;
